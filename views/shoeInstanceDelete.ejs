<%- include('header');%>
<h3><%= shoeInstance.shoe.name %></h3>
<p><%= shoeInstance.stockNumber %></p>
<p><%= shoeInstance.size %></p>
<p>Are you sure you want to delete this Shoe Instance?</p>
<form method="POST" action="">
  <input
    type="hidden"
    name="shoeInstanceID"
    required="true"
    value="<%= shoeInstance._id %>"
  />
  <button type="submit">Delete Shoe</button>
</form>
<%- include('footer');%>
